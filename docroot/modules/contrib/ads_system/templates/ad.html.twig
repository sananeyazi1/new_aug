{#
/**
 * @file ad.html.twig
 * Default theme implementation to present Ad data.
 *
 * This template is used when viewing Ad pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_ad()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('ad') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
